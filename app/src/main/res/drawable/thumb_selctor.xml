<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--滑块的图片,只有前两行能生效,哪个放前面哪个生效-->
    <item android:state_checked="true" android:drawable="@drawable/switch_btn_pressed"/>
    <item android:state_checked="false" android:drawable="@drawable/switch_btn_normal"/>
    <item android:state_pressed="true" android:drawable="@drawable/switch_btn_pressed"/>
    <item android:state_pressed="false" android:drawable="@drawable/switch_btn_normal"/>
</selector>